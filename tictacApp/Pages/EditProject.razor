@page "/projects/{ProjectId:int}/editProject"

@using tictacApp.Data
@using tictacApp.Shared

<EditPlannedActivityComponent 
            TPlannedActivity=Project
            PlannedActivityId=@ProjectId
            Context="Project"
            RootUri="projects"
            Origin=@Origin />

@code {
    [Parameter]
    public int ProjectId { get; set; }

    [Parameter]
    [SupplyParameterFromQuery(Name = "Origin")]
    public string Origin { get; set; } = string.Empty;
}